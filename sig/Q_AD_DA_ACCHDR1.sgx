weboperation GetParams
javascript

	var pClientId = "";
	
    var d_client = uniface.getInstance("Q_QS_DA_QUOTE1").getEntity("DUM.FQS_SYSTEM").getOccurrence(0).getField("D_CLIENTID").getValue();
	if (d_client != null || d_client != "") {
