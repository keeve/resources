operation BuildTablePopup ; Builds all Json for KendoUI
public web
	params
		struct pStruct : IN
	endparams
	variables
		string vFieldList, vField,  vDateString
		String vOuterEntity,    vJFormedField,    vHeaderList,    vOutput,  vTitle, vList, vListTemp, vFld
		String vJsonField,vCellList,  vCellType,  vDateFormat,  vFieldCol, vTemplate, vHidden
		numeric w, x, y,z, i
		string vOuterList, vInnerList
		string pListOptions, pDateTimeFormat, pEntityType, pOuterEntity, pInnerEntity, pPosition, pTitle, pSaveURL, pColumnSyntax, pIDList ,pSaveParams,pToolBar,pErrorDiv     
		numeric pGridNo  , vTotal        
		string vEditable, vFieldInfo , vValidation , vLength , vLenValArr, vItem, vItemID, vIDList , vSaveParams , vAnchorOpen, vAnchorClose   
		string pInstanceName , pPopupFields,pPopupCreateFields,pGridFields,pOpenField , pColumnClass,  vManValArr,pExcel  
		string  pWidthList, vWidthTemplate,  vWidth, vDateField, pExtraLine, pEditTemplate
		boolean vHasRec
		; mantis 1004724 starts
		boolean vFirstInner, vFirstOuter
		numeric vFieldIndex
		string   vComma, vFile, vSetLine, vSetField
		; mantis 1004724 ends
		
	endvariables
	
	; Parameters
	;---------------------------------------------------------------------------------------------
	pListOptions = pStruct-&gt;in-&gt;pListOptions  ;Header List
	pDateTimeFormat = pStruct-&gt;in-&gt;pDateTimeFormat 
	pEntityType = pStruct-&gt;in-&gt;pEntityType 
	pOuterEntity = pStruct-&gt;in-&gt;pOuterEntity 
	pInnerEntity = pStruct-&gt;in-&gt;pInnerEntity 
	pPosition  = pStruct-&gt;in-&gt;pPosition  
	pGridNo = pStruct-&gt;in-&gt;pGridNo 
	pTitle = pStruct-&gt;in-&gt;pTitle       ; Title of the grid/report  
	pSaveURL = pStruct-&gt;in-&gt;pSaveURL   ;URL Which handles save and server validation 
	
	; Associative List containing properties like HID, MAN, LEN(25) for length the number inside the parenthesis is taken
	;example "FIELDNAME = MAN,HID,LEN(50)&uSEP;FIELDNAME2= MAN"
	pColumnSyntax = pStruct-&gt;in-&gt;pColumnSyntax 
	
	pIDList = pStruct-&gt;in-&gt;pIDList  ; list of fieldnames for IDs for composite keys "FIELNAME1&uSEP;FILENAME2
	pSaveParams = pStruct-&gt;in-&gt;pSaveParams ;associative list for save parameters ex compcode=2AANDA will be converted to "conpcode":"2AANDA"
	pToolBar = pStruct-&gt;in-&gt;pToolBar ;Optional - Json string for toolbar 
	pErrorDiv = pStruct-&gt;in-&gt;pErrorDiv ;Optional - div for success and error messages default is #errordiv
	pInstanceName = pStruct-&gt;in-&gt;pInstanceName 
	
	;Associative list containing fields and certain properties to create a popup template
	;&lt;WID&gt;&lt;/WID&gt; = widget type DROP/CHECK. if none textbox is assumed
	;&lt;SHOW&gt;&lt;/SHOW&gt; = name of boolean field to assign visibility ex FIELDNAME=?FIELDNAME1/? fieldname1 dictates fieldname's visibility true is shown
	;&lt;LAB&gt;&lt;/LAB&gt; = label of field
	;&lt;TYPE&gt;&lt;/TYPE&gt; = field type
	;ex "FIELDNAME=&lt;WID&gt;DROP&lt;/WID&gt;&lt;SHOW&gt;BFIELD&lt;/SHOW&gt;&uSEP;FIELDNAME2=&uSEP;FIELDNAME3=&lt;WID&gt;CHECK&lt;/WID&gt;" 
	pPopupFields = pStruct-&gt;in-&gt;pPopupFields 
	pPopupCreateFields = pStruct-&gt;in-&gt;pPopupCreateFields
	pGridFields = pStruct-&gt;in-&gt;pGridFields ;List containing fields to be displayed in the grid, if not specified, all is used
	pOpenField =  pStruct-&gt;in-&gt;pOpenField  ;Field that opens popup window
	pExtraLine = pStruct-&gt;in-&gt;pExtraLine;
	pExcel =  pStruct-&gt;in-&gt;pExcel ;Excel FileName (optional)
	pWidthList=  pStruct-&gt;in-&gt;pWidthList ;specify widh list
	pEditTemplate = pStruct-&gt;in-&gt;pEditTemplate;
	
	;********************************************************************************************
	; Build Painted Fields List based on first outer entity
	; vOuterEntity     = $componentinfo("","OUTERENTITIES")
	; delitem vOuterEntity, 1 ; Remove controls entity 
	vOuterEntity      = pOuterEntity
	vFieldList        = $entinfo(vOuterEntity,"PAINTEDFIELDS")
	vOuterList        = $entinfo(vOuterEntity,"PAINTEDFIELDS")
	if (pGridNo = "")
		pGridNo = 1
	endif
	
	if (pIDList != "")
		forlist vItem in pIDList
			vIDList = $concat(vIDList , '"id":"', vItem, '",') 
		endfor
	endif
	
	if (pEditTemplate = "")
		pEditTemplate = "#edittemplate"
	endif
	
	if (pSaveParams = "")
		pSaveParams = '"Params":"none"'
	else
		forlist/id vItemID, vItem in pSaveParams 
		if (vSaveParams != "") vSaveParams = $concat(vSaveParams, ",")
		vSaveParams = $concat(vSaveParams , '"',vItemID,'":"', $concat(vItem ,'"'))                                     
	endfor
	pSaveParams = vSaveParams 
endif
pSaveParams = $concat("{",pSaveParams ,"}")

pPopupFields = $replace(pPopupFields ,1,"&uSEP;", "|&amp;|", -1)
pPopupCreateFields = $replace(pPopupCreateFields,1,"&uSEP;", "|&amp;|", -1)

vCellList = '"fields":{'
x = $itemcount(vFieldList)
forList vField, i in vfieldList                    
	if ($scan($uppercase($item(vField,pColumnSyntax)), "NED") &gt; 0) 
		vEditable = ',"editable":false'            
	else
		vEditable = ""
	endif        
	
	vValidation = ""
	if ($scan($uppercase($item(vField,pColumnSyntax)), "MAN") &gt; 0) 
		vValidation = '"required": true'
		if (vManValArr != "")  vManValArr = $concat( vManValArr  , "|&amp;|")
		vManValArr  = $concat( vManValArr ,vField )               
	endif
	if ($scan($uppercase($item(vField,pColumnSyntax)), "LEN") &gt; 0) 
		if (vValidation != "") vValidation = $concat(vValidation , ",") 
		vLength =   $item(vField,pColumnSyntax)[$scan($uppercase($item(vField,pColumnSyntax)), "LEN")]
		vLength = vLength[5,$scan(vLength,")") -1] 
		vValidation = $concat(vValidation,'"lengthvalidation":"', vLength, '"') 
		if (vLenValArr != "")  vLenValArr = $concat( vLenValArr , "|&amp;|")
		vLenValArr = $concat( vLenValArr ,vField )   
	endif
	if (vValidation != "") vValidation  = $concat(',"validation": {',vValidation, "}")                            
	vEditable  = $concat(vEditable  , vValidation ) 
	
	vCellType = $fieldinfo(vField,"DATATYPE")
	selectcase vCellType
		case "Numeric"
			vCellType = "number"
		case "String"
			vCellType = "string"
		case "Date", "Datetime", "Time", "LinearDatetime"
			vCellType = "date"
			putitem vDateField, -1, vField
			;Paul
			;elsecase
			;filedump "%%vCellType%%%", "C:\Users\dcarrasco\Documents\List.txt" 
	endselectcase
	vCellList = $concat(vCellList,$concat('"',vField,'"'),":",'{"type": ',$concat('"',vCellType,'"', vEditable  , '}'))
	if (i != x)
		vCellList     = $concat(vCellList,",")
	endif
endfor  

if (pInnerEntity != "")
	w = 0
	forentity pInnerEntity
		w = w + 1
		forlist vField, x in pInnerEntity
			
			y = 1
			while y &lt;= $itemcount($entinfo(vField,"PAINTEDFIELDS"))
				getitem(vFld,$entinfo(vField,"PAINTEDFIELDS"),y)
				putitem vInnerList, -1, vFld 
				
				;inner fields set to hidden are not considered
				if ($scan($uppercase($item(vFld,pColumnSyntax)), "HID") = 0) 
					putitem vFieldList, -1, $concat(vFld,w) ;put field in the list                         
					
					vEditable = ""
					if ($scan($uppercase($item(vFld,pColumnSyntax)), "NED") &gt; 0) 
						vEditable = ',"editable":false'    
					endif  
					
					vValidation = ""
					if ($scan($uppercase($item(vFld,pColumnSyntax)), "MAN") &gt; 0) 
						vValidation = '"required": true'
					endif
					if ($scan($uppercase($item(vFld,pColumnSyntax)), "LEN") &gt; 0) 
						if (vValidation != "") vValidation = $concat(vValidation , ",") 
						vLength =   $item(vFld,pColumnSyntax)[$scan($uppercase($item(vFld,pColumnSyntax)), "LEN")]
						vLength = vLength[5,$scan(vLength,")") -1] 
						vValidation = $concat(vValidation,'"lengthvalidation":"', vLength, '"') 
						if (vLenValArr != "")  vLenValArr = $concat( vLenValArr , "|&amp;|")
						vLenValArr = $concat( vLenValArr ,vFld )   
					endif
					if (vValidation != "") vValidation  = $concat(',"validation": {',vValidation, "}")                            
					vEditable  = $concat(vEditable  , vValidation ) 
					
					vCellType = $fieldinfo(vFld,"DATATYPE")
					selectcase vCellType
						case "Numeric"
							vCellType = "number"
						case "String"
							vCellType = "string"
						case "Date", "Datetime", "Time", "LinearDatetime"
							vCellType = "date"
							putitem vDateField, -1, vField
							;Paul
					endselectcase
					vCellList = $concat(vCellList,",")
					vCellList = $concat(vCellList, $concat('"',vFld,w,'"'),":",'{"type": ', $concat('"',vCellType,'"', vEditable  , '}'))
				endif
				y += 1
			endwhile                
		endfor        
	endfor
	sort/list vInnerList, "U" 
endif

vCellList = $concat("{",vIDList , vCellList,"}}")
;********************************************************************************************
;(Re)Initialise Variables
vField = ""
x=0
y=0
i=0
;********************************************************************************************
;Set the Kendo date format
selectcase pDateTimeFormat
	case "FDF"
		vDateFormat    = "{0:dd-MMM-yyyy hh:mm:ss tt}"
	case "SDF"
		vDateFormat    = "{0:dd-MMM-yyyy}"
	case "TFO"
		vDateFormat    = "{0:hh:mm:ss tt}"
endselectcase
;********************************************************************************************
;Build the Kendo column formats


vHeaderList     = "["
if (pGridFields = "")
	pGridFields =vfieldList
endif
x = $itemcount(pGridFields) 
forList vField, i in pGridFields 
	if (pOpenField != "" &amp; pOpenField = vField)
		vAnchorOpen = "&lt;a style='cursor:pointer;' onclick='openPopup(this);'&gt;"
		vAnchorClose = "&lt;/a&gt;"
	else
		vAnchorOpen = ""
		vAnchorClose = ""
	endif 
	
	if (pListOptions != "") vTitle = $concat(',','"title":"',$itemnr(i,pListOptions),'"')
	
	if ($scan($uppercase($item(vField,pColumnSyntax)), "HID") &gt; 0) 
		vHidden = ",%%"hidden%%":%%"true%%""
	else
		vHidden = ""
	endif
	
	
	if ($item(vField , pWidthList) != "")
		vWidth = $item(vField , pWidthList)
		vWidthTemplate= ",%%"width%%":%%"%%vWidth%%%px%%""
	else
		vWidthTemplate = ""
	endif
	
	selectcase $fieldInfo(vField,"DATATYPE") 
		case "Date"
			vTemplate = $concat(",%%"template%%":%%"&lt;span style ='text-transform: uppercase;'&gt;#=",vField," != 'null' ? kendo.toString(",vField,", 'dd-MMM-yyyy') : '' #&lt;/span&gt;%%"")
			vHeaderList     = $concat(vHeaderList,'{"field":"',vField,'","format":"{0:dd-MMM-yyyy}"',$concat(vTemplate,vHidden ,vTitle,vWidthTemplate ,'}'))            
		case "Datetime",  "LinearDatetime"
			vTemplate = $concat(",%%"template%%":%%"&lt;span style ='text-transform: uppercase;'&gt;#=",vField," != 'null' ? kendo.toString(",vField,", 'dd-MMM-yyyy HH:MM') : '' #&lt;/span&gt;%%"")
			;                vHeaderList     = "%%vHeaderList%%%{%%"field%%":%%"%%vField%%%%%",%%"format%%":%%"{0:dd-MMM-yyyy hh:mm:ss tt}%%", %%"parseFormats%%": [%%"MMM yyyy%%",%%"hh:mm%%"]%%vTemplate%%%%%vHidden%%%%%vTitle%%%}"            
			vHeaderList     = $concat(vHeaderList,'{"field":"',vField,'","format":"{0:dd-MMM-yyyy}"',$concat(vTemplate,vHidden ,vTitle,vWidthTemplate ,'}'))            
		case "Time"
			vHeaderList     = $concat(vHeaderList,'{"field":','"',vField,$concat('","format":"{0:hh:mm:ss tt}"',vTitle,vHidden,vWidthTemplate ,'}'))
			;Paul
		case "Boolean"    
			vHeaderList = $concat(vHeaderList,'{"field":','"',vField,$concat('"',vTitle,vHidden,vWidthTemplate ))
			vHeaderList = $concat(vHeaderList, $concat(',"template":','"',"&lt;input type='checkbox' /&gt;",'"','}'))
		case "Numeric"
			vTemplate = $concat(",%%"template%%":%%"&lt;span style ='float: right;'&gt;#=",vField," != 'null' ? kendo.toString(",vField,", 'n0') : '' #&lt;/span&gt;%%"")
			vHeaderList = $concat(vHeaderList,'{"field":','"',vField,$concat('"',vTemplate,vHidden,vTitle,$concat(vWidthTemplate ,'}')))            
		case "Float"
			vTemplate = $concat(",%%"template%%":%%"&lt;span style ='float: right;'&gt;#=",vField," != 'null' ? kendo.toString(",vField,", 'n') : '0' #&lt;/span&gt;%%"")
			vHeaderList = $concat(vHeaderList,'{"field":','"',vField,$concat('"',vTemplate,vHidden,vTitle,$concat(vWidthTemplate ,'}')))            
		elsecase
			
			vTemplate = $concat(",%%"template%%":%%"&lt;span&gt;",vAnchorOpen, "#=",vField,$concat("#",vAnchorClose ,"&lt;/span&gt;%%""))
			vHeaderList = $concat(vHeaderList,'{"field":','"',vField,$concat('"',vTemplate,vHidden,vTitle,$concat(vWidthTemplate ,'}')))            
	endselectcase
	if (i != x)
		vHeaderList = $concat(vHeaderList,",")
	endif
endfor
vHeaderList     = $concat(vHeaderList,"]")
;********************************************************************************************
;(Re)Initialise Variables
vField = ""
x=0
y=0
i=0
;********************************************************************************************
;{Retrieve data from database and} build grid
;    
;==================================================
; mantis 1004724 replaces whole of old section
vFile       = $concat($concat($logical("DATA-LOCATION"),$logical("DATA-TEMP")),&lt;usercode&gt;,vOuterEntity,$datim,"_griddumpfile.txt")
vFirstInner = 1
vFirstOuter = 1
filedump "[", vFile
vHasRec = "F"
forentity vOuterEntity
	if (vFirstOuter) 
		vFirstOuter = 0
		vSetLine = "{"
	else
		vSetLine = "},{"
	endif
	forList vField, vFieldIndex in vOuterList
		;check if all records are blank
		if (@vfield != "")
			vHasRec = "T"
		endif
		
		vSetField = ""
		vJsonField = $concat('"',@vField,'"')
		if (vFieldIndex != 1)
			vComma = ","
		else
			vComma = ""
		endif
		vJFormedField = $concat('"',vField,'"')
		selectcase $fieldInfo(vField,"DATATYPE")
			case "Date"
				vDateString    = $concat(@vField[5:2],"/",@vField[7:2],$concat("/",@vField[1:4]))
				vSetField      = $concat(vComma,vJFormedField,":",$concat('"',vDateString,'"'))
			case "Datetime","LinearDatetime"
				vDateString    = $concat(@vField[5:2],"/",@vField[7:2],$concat("/",@vField[1:4]))
				vSetField      = $concat(vComma,vJFormedField,":",$concat('"',vDateString,'"'))
			case "Boolean"
				if (@vField    = "T") 
					vSetField  = $concat(vComma,vJFormedField,":%%"true%%"")
				else
					vSetField  = $concat(vComma,vJFormedField,":%%"false%%"")
				endif
			case "Numeric"
				vJsonField     = @vfield
				if (@vField    = "") @vField = 'null'
				vSetField      = $concat(vComma,vJFormedField,":",@vField)
			case "String", "Raw"
				vSetField      = $concat(vComma,vJFormedField,":",$concat('"',$replace(@vField,1,'"','\"',-1),'"'))
			elsecase
				if (@vField    = "") @vField = 'null'
				vSetField      = $concat(vComma,vJFormedField,":",@vField)
		endselectcase
		vSetLine = $concat(vSetLine, vSetField)
	endfor
	if (pInnerEntity != "")
		w = 0 
		forentity pInnerEntity
			vSetLine = $concat(vSetLine,",")
			w += 1
			forList vField, vFieldIndex in vInnerList
				vSetField = ""
				vJsonField = $concat('"',@vField,'"')
				if (vFieldIndex != 1)
					vComma = ","
				else
					vComma = ""
				endif
				vJFormedField = $concat('"',vField,w,'"')
				selectcase $fieldInfo(vField,"DATATYPE")
					case "Date"
						vDateString    = $concat(@vField[5:2],"/",@vField[7:2],$concat("/",@vField[1:4]))
						vSetField      = $concat(vComma,vJFormedField,":",$concat('"',vDateString,'"'))
					case "Datetime","LinearDatetime"                        
						vDateString    = $concat(@vField[5:2],"/",@vField[7:2],$concat("/",@vField[1:4]))
						vSetField      = $concat(vComma,vJFormedField,":",$concat('"',vDateString,'"'))
					case "Boolean"
						if (@vField    = "T") 
							vSetField  = $concat(vComma,vJFormedField,":%%"true%%"")
						else
							vSetField  = $concat(vComma,vJFormedField,":%%"false%%"")
						endif
					case "Numeric"
						vJsonField     = @vfield
						if (@vField    = "") @vField = 'null'
						vSetField      = $concat(vComma,vJFormedField,":",@vField)
					case "String", "Raw"
						vSetField      = $concat(vComma,vJFormedField,":",$concat('"',$replace(@vField,1,'"','\"',-1),'"'))
					elsecase
						if (@vField    = "") @vField = 'null'
						vSetField      = $concat(vComma,vJFormedField,":",@vField)
				endselectcase
				
				vSetLine = $concat(vSetLine, vSetField)
				
			endfor
		endfor
	endif        
	filedump/append vSetLine, vFile
endfor

if (vFirstOuter)
	FileDump/append "]",vFile
else
	FileDump/append "}]",vFile
endif

if (vHasRec)
	vTotal = $totocc(vOuterEntity)
else
	vTotal = 0
endif

vDateField = $replace(vDateField,1,"&uSEP;",",",-1)    
;********************************************************************************************
;Send Data To Grid
; mantis 1004724 starts
fileload vFile, vOutput
;    vOutput = $replace (vOutput,1,"%%^","",-1)

; mantis 1004724 ends
vOutput= $concat('{"data":',vOutput, ',"total":', "%%vTotal%%%}")
vOutput= $replace (vOutput,1,"%%^","",-1)
if (vOutput = "" | vHeaderList = "" | vCellList = "")
	$collHandle(MESSAGELINE)-&gt;showMessage("error","Error:","Unable to generate data for grid",1,0)
else
	webactivate $instancename.BuildGridPopup(vOutput,vHeaderList,vCellList, pPosition,pGridNo,pTitle,vLenValArr,vManValArr , %\
	pSaveURL,pSaveParams,pToolBar,pErrorDiv,pInstanceName,pPopupFields,pPopupCreateFields,pExcel, vDateField,pExtraLine, pEditTemplate)
endif
end ;BuildDataPopup
;#######################################################################################################



weboperation BuildGridPopup
params
	String pJsonData    : In
	String pTableHeader : In
	String pFields      : In
	string pPosition        : IN
	numeric    pGrid : IN
	string pTitle : IN
	string pLenValArr : IN  ;An Array consisting of fields with length validation
	string pManValArr : IN
	string pSaveURL : IN
	string pSaveParams : IN 
	string pToolBar : IN
	string pErrorDiv : IN
	string pInstanceName  : IN
	string pPopupFields : IN
	string pPopupCreateFields : IN
	string pExcel : IN
	string pDateField : IN
	string pExtraLine : IN
	string pEditTemplate : IN
endparams
javascript

console.log(pTableHeader);
console.log(pJsonData);
console.log(pFields);
console.log(pSaveParams);
console.log(pSaveURL);

var GridName = '#grid' + pGrid
var GridName2 = 'grid' + pGrid

if (!jQuery.contains(document, $(GridName)[0])) {
$(pPosition).append("&lt;div id='"+ GridName2 + "'&gt;&lt;/div&gt;")
}


var data    = $.parseJSON(pJsonData);
var headerList     = $.parseJSON(pTableHeader);
var fieldList     = $.parseJSON(pFields);
var vSaveParams = $.parseJSON(pSaveParams);
var vSaveURL = pSaveURL; 
var vErrorDiv = pErrorDiv;
var vInstanceName = pInstanceName ; 



if (vSaveURL == ""){
vSaveURL = "save";
}
if (vErrorDiv == ""){
vErrorDiv = "#errordiv";
}

if (pExcel == ""){
pExcel = "Report Extract"
}


var opt_sortable = false;
window.init = false;


if (pLenValArr != ""){
var name = "" , vLen = "";
var vLenArray = pLenValArr.split("|&amp;|");
vLenValues = [];
for (i = 0; i &lt; vLenArray.length; i++) { 
	name  = vLenArray[i];
	vLen = fieldList["fields"][name]["validation"]["lengthvalidation"];
	vLenValues[name] = vLen; 
	fieldList["fields"][name]["validation"]["lengthvalidation"] = function (input){
	var vField = input.attr("name");
	var vLength = vLenValues[vField];
	if (input.val().length &gt; vLength ){
	input.attr("data-lengthvalidation-msg", "Field too large. Max(" + vLength + " characters)");
	return false;
	} return true;
	};
	}
	}
	
	//;clear template
	$("body").find($("#edittemplate")).remove();
	//;build kendo grid popup template string
	var editPopupTemplate = createCreateEditTemplate(pPopupFields); //;this is for edit mode
	var createPopupTemplate = createCreateEditTemplate(pPopupCreateFields); //;this is for create mode
	var extraLine = (pExtraLine != "") ? " &lt;br /&gt;" + pExtraLine : "";
	
	var vPopup = '&lt;script type="text/x-kendo-template" id="edittemplate"&gt;' + 
	' # if(data.isNew()) { # ' +
	'&lt;table width="550" id="tblCreate" style=" margin-left:25px;margin-right:25px;"&gt;&lt;tbody&gt;' + createPopupTemplate + '&lt;/tbody&gt;&lt;/table&gt;' +    
	' # } else { # ' +
	'&lt;table width="550" id="tblEdit" style=" margin-left:25px;margin-right:25px;"&gt;&lt;tbody&gt;' + editPopupTemplate + '&lt;/tbody&gt;&lt;/table&gt;' +    
	' # } #' +              
	'&lt;/script&gt;';    
	$("body").append(vPopup);
	
	openPopup = function (element){
	var row = $(element).closest("tr");
	var gridId = $(element).closest(".k-grid").attr("id");
	$("#" + gridId).data("kendoGrid").editRow(row);
	}
	
	
	$(GridName).empty();
	$(GridName).kendoGrid({   
	columns: headerList ,
	dataSource:{ 
	transport: {
	read: function(options) { 
	options.success(data);
	},
	create: saveTransport,
	update: saveTransport
	},        
	batch: true,
	schema: {     
	data: "data",
	total: "total",      
	model: fieldList
	}
	},
	height:           "100%",
	sortable:         true,
	pageable: {
	pageSize: 500,
	pageSizes: [10, 20, 50, 100, 500, "ALL"]
	},
	editable: {
	mode: "popup",
	template: $(pEditTemplate).html()
	},
	edit: function (e) 
	{
	var editWindow = this.editable.element.data("kendoWindow");
	editWindow.wrapper.css({ width: 600 , "left": "50%", "margin-left": "-300px"});
	if (e.model.isNew()) {
	$('.k-window-title').text("Create Details");
	} else {                
	$('.k-window-title').text("Modify Details");
	}
	$('.k-window-titlebar').css({background:"#337ab7", height: "35px", "position":"relative", "color": "#fff", "font-size": "30px", "text-align": "center"});
	$('.k-edit-form-container').css({width: 600 });
	},
	dataBound: function(e) {
	
	$("td").each(function() {    
	if ($(this).text() == "*HIDE") {
	$(this).next("td").addClass("hidden")
	}
	});
	$("th").each(function() {    
	if ($(this).text() == "Hide?") {
	$(this).next("th").addClass("hidden")
	}
	});
	$("td").each(function() {    
	if ($(this).text() == "A") {
	$(this).next("td").addClass("fscgreen")
	}
	});    
	$("td").each(function() {
	if ($(this).text() == "P") {
	$(this).next("td").addClass("fscred")
	}
	});
	
	var dataView = this.dataSource.view();
	for (var i = 0; i &lt; dataView.length; i++) {
		if (dataView[i].D_ROW!=null) {
		var uid = dataView[i].uid;
		$(GridName + " tbody").find("tr[data-uid=" + uid + "]").addClass(dataView[i].D_ROW);  //alarm's in my style and we call uid for each row
		}
		}
		
		if ($(GridName).data("kendoGrid").dataSource.data().length &gt; 0) {    
		$(GridName).removeClass("hidden");        
		} else {
		//$(GridName).addClass("hidden");    
		//modalPopup("EmptyTable","No records found?","No data available for current criteria selected or too many records to return.  Please try reducing your selection criteria.")
		$(GridName  + ".k-update").parent().hide();
		$("#kendoDelete").hide();
		var grid = e.sender;
		var colCount = grid.columns.length;
		$(e.sender.wrapper)
		.find('tbody')
		.append('&lt;tr class="kendo-data-row"&gt;&lt;td colspan="' + colCount + '" class="no-data"&gt;&lt;div class="subHeader" style="margin-top:50px;color:gray;font-size:20px;text-align:center;"&gt;No Records Found&lt;/div&gt;&lt;/td&gt;&lt;/tr&gt;');
		};
		
		
		$(".spinner").fadeOut("fast");  
		if (window.init == false){
		window.init = true;
		$(GridName).data("kendoGrid").refresh()
		}
		}
		});
		
		if($(GridName).find("h1").length &lt; 1) {
		$(".k-grid-toolbar", GridName).append("&lt;h1&gt;" + pTitle + "&lt;/h1&gt;");
		};
		
		function saveTransport(options) {
		var dt = "";
		$.each(options.data.models, function (index, elem) {
		$.each(elem, function(id, val) {
		if (pDateField.indexOf(id) &gt;= 0) {
		dt = new Date(val);
		elem[id] = kendo.toString(new Date(dt), "dd-MM-yyyy");
		}
		});
		
		});
		$.ajax( {
		url: vSaveURL  ,
		dataType:"json",
		data: {
		models: kendo.stringify(options.data.models), saveparams: vSaveParams
		},
		success: function(result) {
		options.success(result);
		console.log(result);
		var vSeverity = result.severity;
		var vTitle = result.title;
		var vIcon= result.icon;
		var vMessage = result.message;
		
		if (vSeverity == "") vSeverity = "danger";
		if (vTitle == "") vTitle = "Danger!";
		if (vIcon == "") vIcon = "exclamation-sign";
		if (vMessage == "") vMessage = "Procedure Failed. Plese Contact the Administrator.";
		
		$(vErrorDiv ).append('&lt;div class="alert alert-' + vSeverity + 
		'"&gt;&lt;span class="glyphicon glyphicon-' + vIcon  + '" aria-hidden="true"&gt;&lt;/span&gt;' +
		'&amp;nbsp;&lt;button type="button" class="close" data-dismiss="alert" aria-label="close"&gt;&amp;times;&lt;/button&gt;&lt;strong&gt;' +
		vTitle  + '&lt;/strong&gt;&amp;nbsp;&amp;nbsp;' + vMessage  + '&lt;/div&gt;')
		$(".spinner").fadeIn("fast"); 
		uniface.getInstance(vInstanceName).activate("LoadGrid");
		
		},
		
		});
		}
		
		function createCreateEditTemplate(popupFields){
		
		console.log(popupFields);
		var vManValArr = pManValArr.split("|&amp;|")
		
		var vPopupFields = popupFields.split("|&amp;|");
		var vInnerPopup = "", vElement = "", vElemOuter = "", vField = "", vName = "", vLabel = "", vType= "", vRequired = "";
		var vToggleFld = "", vWid ="";
		
		for (i = 0; i &lt; vPopupFields.length; i++) {
			vField = vPopupFields[i];
			if (vField != ""){
			
			vName = vField.substring(0,vField.search("="));
			vElement="";
			vLabel = "";
			vRequired = "";
			
			if (vManValArr.indexOf(vName) &gt;= 0){ 
			vRequired = "required"
			}
			
			if (vField.search("&lt;LAB&gt;") &gt;= 0 )    {
			vLabel = vField.substring(vField.search("&lt;LAB&gt;") + 5, vField.search("&lt;/LAB&gt;")); 
			}
			if (vField.search("&lt;TYPE&gt;") &gt;= 0 )    {
			vType= 'data-type="' + vField.substring(vField.search("&lt;TYPE&gt;") + 6, vField.search("&lt;/TYPE&gt;")) + '"';
			}
			
			vWid = vField.substring(vField.search("&lt;WID&gt;") + 5, vField.search("&lt;/WID&gt;"));
			
			//;Build Element
			if (vField.search("&lt;WID&gt;") &gt;= 0 ) {
			//; dropbox
			if ( vWid.search("DROP") &gt;= 0){
			var DropParams = vWid.split(",");
			var Datasource = DropParams[1];
			vElement = "&lt;tr&gt;&lt;td width='230'&gt;" + vLabel  + "&lt;/td&gt;" + 
			"&lt;td width='320'&gt;" +        
			'&lt;input  ' + vRequired + '  class="k-input" style="width:320px;" name="' + vName + '" ' +
			'data-bind="value:' + vName + '" ' + 
			'data-value-field="' + vName + 'ID" ' +
			'data-text-field="' + vName + 'VAL" ' +
			'data-source="' + Datasource + '" ' + 
			'data-role="dropdownlist" /&gt;'    +    
			"&lt;/td&gt;&lt;/tr&gt;";
			}else if ( vWid.search("DRPVIR") &gt;= 0){
			var DropParams = vWid.split(",");
			var Datasource = DropParams[1];
			var vAlternateName = DropParams[2];
			if (typeof vAlternateName === 'undefined') {
			vAlternateName = vName;
			}
			vElement = "&lt;tr&gt;&lt;td width='230'&gt;" + vLabel  + "&lt;/td&gt;" + 
			"&lt;td width='320'&gt;" +        
			'&lt;input ' + vRequired + ' class="k-input" style="width:320px;" name="' + vName + '" ' +
			'data-bind="value:' + vName + '" ' + 
			'data-value-field="' + vAlternateName + 'ID" ' +
			'data-text-field="' + vAlternateName + 'VAL" ' +
			'data-source="' + Datasource + '" ' + 
			'data-filter = "contains" ' + 
			'data-virtual = "{itemHeight: 26,valueMapper: function(options) {options.success()}}" ' +
			'data-role="dropdownlist" /&gt;'    +    
			"&lt;/td&gt;&lt;/tr&gt;";
			
			//; checkbox    
			}else if ( vWid.search("SWITCH") &gt;= 0){
			vElement = "&lt;tr&gt;&lt;td width='40%'&gt;" + vLabel  + "&lt;/td&gt;" +
			'&lt;td &gt;&lt;div class="onoffswitch" &gt;' + 
			'&lt;input type="checkbox"  class="onoffswitch-checkbox" id="' + vName + 
			'" name="' + vName + '" data-bind="value:' + vName + '" /&gt;' +
			'&lt;label class="onoffswitch-label" for="' + vName + '" &gt;' +
			'&lt;span class="onoffswitch-inner"&gt;&lt;/span&gt;' +
			'&lt;span class="onoffswitch-switch"&gt;&lt;/span&gt;' +
			'&lt;/label&gt;' +
			"&lt;/div&gt;&lt;/td&gt;&lt;/tr&gt;";
			
			}else if ( vWid.search("CHECK") &gt;= 0){
			vElement =   "&lt;tr&gt;&lt;td width='40%'&gt;" + vLabel  + "&lt;/td&gt;" +
			"&lt;td &gt;" +                 
			'&lt;input type="checkbox"  class="k-input" name="' + vName + '" data-bind="value:' + vName + '" style="height:25px;"&gt;' + 
			"&lt;/td&gt;&lt;/tr&gt;";
			
			}else if ( vWid.search("TEXTREAD") &gt;= 0){
			vElement =   "&lt;tr&gt;&lt;td width='40%'&gt;" + vLabel  +  "&lt;/td&gt;" +
			'&lt;td width="60%"&gt;&lt;input ' + vRequired + ' type="text" class="k-input k-textbox" name="' + vName + '" ' + 
			vType  + ' data-bind="value:' + vName + '" style="width:100%;" readonly/&gt;' + 
			"&lt;/td&gt;&lt;/tr&gt;";
			
			} else if (vWid.search("DATEPICKER") &gt;= 0) {
			vElement =   "&lt;tr&gt;&lt;td width='40%'&gt;" + vLabel  +  "&lt;/td&gt;" +
			'&lt;td width="60%"&gt;&lt;input ' + vRequired + ' type="text" class="k-input k-textbox" name="' + vName + '" ' + 
			vType  + ' data-bind="value:' + vName + '" data-role="datepicker" data-format="{0:dd-MMM-yyyy}" style="width:100%;" /&gt;' + 
			"&lt;/td&gt;&lt;/tr&gt;";
			} else if (vWid.search("NUMERIC") &gt;= 0) {
			vElement =   "&lt;tr&gt;&lt;td width='40%'&gt;" + vLabel  +  "&lt;/td&gt;" +
			'&lt;td width="60%"&gt;&lt;input ' + vRequired + ' type="text" class="k-input k-textbox" name="' + vName + '" ' + 
			vType  + ' data-bind="value:' + vName + '" data-role="numerictextbox" style="width:100%;" /&gt;' + 
			"&lt;/td&gt;&lt;/tr&gt;";
			} else if (vWid.search("STATIC") &gt;= 0) {
			var vStatParams = vWid.split(",");
			var vText = vStatParams[1];
			vElement =   "&lt;tr&gt;&lt;td width='40%'&gt;" + vLabel  +  "&lt;/td&gt;" +
			"&lt;td width='60%'&gt;" + vText  + 
			"&lt;/td&gt;&lt;/tr&gt;";
			}
			
			}else{
			//;textbox element
			vElement = "&lt;tr&gt;&lt;td width='40%'&gt;" + vLabel  +  "&lt;/td&gt;" +
			'&lt;td width="60%"&gt;&lt;input ' + vRequired + ' type="text" class="k-input k-textbox" id="' + vName +  '" name="' + vName + '" ' + 
			vType  + ' data-bind="value:' + vName + '" style="width:100%;"/&gt;' + 
			"&lt;/td&gt;&lt;/tr&gt;";
			
			}
			//;Apply Javascript for element visibility
			if (vField.search("&lt;SHOW&gt;") &gt;= 0 ) {
			vToggleFld = vField.substring(vField.search("&lt;SHOW&gt;") + 6, vField.search("&lt;/SHOW&gt;")); 
			vElement = '# if (' + vToggleFld + ' == true){#' + vElement + '# } #';    
			}
			
			vInnerPopup = vInnerPopup + vElement ;
			}
			}
			
			console.log(vInnerPopup);
			return vInnerPopup;
			}
			
			endjavascript    
		end
		
		weboperation console
		params
			string pIn : In
		endparams
		javascript
		console.log(pIn)
		endjavascript
	end 
	</DAT>
