<DAT name="SFUNC" xml:space='preserve'>operation logon 
	params
		struct        pParams        : INOUT
	endParams
	variables
		boolean vPasswordExpired, vInactive
		string  vUserCode, vUserName, vPassword, vCurrPassword, vCookieDetails, vDefaultCompany, vParent, vClient, vCompName, vPartnerStatus
		numeric vLoginStatus, vStatus, vCompType
		struct  vStruct
	endvariables
	
	vUserCode = pParams-&gt;in-&gt;usersCode
	vPassword = pParams-&gt;in-&gt;password
	
	if (vUserCode = "" | vPassword = "")
		pParams-&gt;out-&gt;Status = -99
		return(0)
	endif
	
;	vLoginStatus settings
;	=====================
;	  0 = successful
;	 -1 = no default company set
;	 -2 = company not enabled
;	 -3 = user not enabled
;    -4 = User not yet verified
;	-10 = incorrect password
;	-97 = can't create cookie
;	-98 = other issue
;	-99 = unknown user

	vLoginStatus = -99	; unknown user

	clear/e "CN_USER"
	USER_CODE.CN_USER = vUserCode
	retrieve/e "CN_USER"
	if ($procerror = &lt;UACT_SUCCESS&gt;)
		vLoginStatus = 0
	else
		clear/e "CN_USER" 
		PRIMARY_EMAIL.CN_USER = vUserCode
		retrieve/e "CN_USER"
		if ($procerror = &lt;UACT_SUCCESS&gt;)
			vLoginStatus = 0
		else
			clear/e "CN_USER"
			USER_ID.CN_USER = vUserCode
			retrieve/e "CN_USER"
			if ($procerror = &lt;UACT_SUCCESS&gt;)
				vLoginStatus = 0
			endif
		endif
	endif

	if (vLoginStatus = 0 &amp; !ENABLED.CN_USER)
		vLoginStatus = -3
	endif

	if (vLoginStatus = 0 &amp; PENDING.CN_USER = 1)
		vLoginStatus = -4
	endif

	retrieve/e "CN_USER_DEFAULTS"

	getitem vParent, DEFAULT_VALUE.CN_USER_DEFAULTS, 1
	getitem vClient, DEFAULT_VALUE.CN_USER_DEFAULTS, 2
	if (vLoginStatus = 0 &amp; vParent = "")
		vLoginStatus = -1
	endif

	#include IFC:INITSTRUCTPARAM

	if (vLoginStatus = 0 &amp; vClient = "") 
		vStruct-&gt;in-&gt;UUID = vParent
		activate "C_AD_SA_COMPANY1".getCompanyInfo(vStruct)
		vCompName = vStruct-&gt;out-&gt;CompanyName
		vCompType = vStruct-&gt;out-&gt;CompanyType
		vPartnerStatus = $item("PARTNER_STATUS", vStruct-&gt;out-&gt;Info)
		if (vStruct-&gt;out-&gt;Enabled != "1")
			vLoginStatus = -2
		endif
	endif

	if (vLoginStatus = 0 &amp; vClient != "")
		vCompName = ""
 
		;identity if vClient is a company or client
		vStruct-&gt;in-&gt;UUID = vClient 
		activate "C_AD_SA_COMPANY1".getCompanyInfo(vStruct)
		vCompName = vStruct-&gt;out-&gt;CompanyName
		vCompType = vStruct-&gt;out-&gt;CompanyType
		vPartnerStatus = $item("PARTNER_STATUS", vStruct-&gt;out-&gt;Info)

		if (vStruct-&gt;out-&gt;Enabled != "1")
			vLoginStatus = -2
		endif
 
		if (vCompName = "") ;if client
			vStruct-&gt;in-&gt;ParentUUID  = vParent
			vStruct-&gt;in-&gt;UUID = vClient
			
			activate "C_AD_SA_COMPANY1".getClientInfo(vStruct)
			vCompName = vStruct-&gt;out-&gt;CompanyName
			vCompType = vStruct-&gt;out-&gt;CompanyType
			if (vStruct-&gt;out-&gt;Enabled != "1")
				vLoginStatus = -2
			endif
		endif
		
	endif

	vCurrPassword = decodePassword(PASSWORD.CN_USER)
	if (vLoginStatus = 0 &amp; $uppercase(vCurrPassword) != $uppercase(vPassword)) 
		vLoginStatus = -10
	endif

	if (vLoginStatus = 0) 
		vPasswordExpired = lpPasswordExpired(USER_CODE.CN_USER,encodePassword(vPassword))
	endif

	pParams-&gt;out-&gt;Status            = vLoginStatus
	pParams-&gt;out-&gt;Inactive          = ENABLED.CN_USER
	pParams-&gt;out-&gt;PWExpired         = vPasswordExpired
	pParams-&gt;out-&gt;cookieDetails     = vCookieDetails
	pParams-&gt;out-&gt;userName          = $concat(FIRST_NAME.CN_USER," ",LAST_NAME.CN_USER)
	pParams-&gt;out-&gt;userscode         = vUserCode
	pParams-&gt;out-&gt;usercode          = USER_ID.CN_USER
	pParams-&gt;out-&gt;Parent            = vParent
;	pParams-&gt;out-&gt;Parent            = "SYSTEM"
	pParams-&gt;out-&gt;Client            = vClient
	pParams-&gt;out-&gt;CompanyType       = vCompType
	pParams-&gt;out-&gt;CompanyName       = vCompName
	pParams-&gt;out-&gt;PartnerStatus	  = vPartnerStatus 
	if (vLoginStatus = 0)
		call lpCreateCookie(pParams)
	endif

	return (0)
	
end


operation changePassword
	params
		string        pIn        : IN
		string        pOut    : OUT
	endparams
	variables
		string        vUserCode, vPassword
	endvariables
	
	vUserCode = $item("userCode", pIn)
	vPassword = $item("password", pIn)
	
	clear/e "CN_USER"
	USER_CODE.CN_USER = vUsercode
	retrieve/e "CN_USER"
	if($procerror = &lt;UACT_SUCCESS&gt;)
		PASSWORD.CN_USER = encodePassword($uppercase(vPassword))
		PASSWORD_LAST_UPDATED.CN_USER  = $datim 
		if(!lpPasswordUsedBefore(vUserCode, vPassword))
			store/e "CN_USER"
			if($procerror = &lt;UACT_SUCCESS&gt;)
				putitem/id pOut, "Store Status",0
				commit
				call lpCreatePasswordHistory()
			else                
				putitem/id pOut, "Store Status", $procerror
				rollback
			endif        
			putitem/id pOut, "Retrieve Status", $procerror
		else
			putitem/id pOut, "Message",getMessage("SY_028","")
		endif
	endif
	
	return (0)
	
end


operation sendPWResetRequest
	params
		string        pIn        : IN
		string        pOut    : OUT
	endparams
	variables
		string     vUserCode, vCompCode, vUserEmail
		boolean    vCompValid
	endvariables
	
	vUserCode  = $lowercase($item("UserCode",  pIn))
	vCompCode  = $lowercase($item("CompCode",  pIn))
	vUserEmail = $lowercase($item("UserEmail", PIn))
	vCompValid = 0
	
	if(vUserCode = "" | vCompCode = "" | vUserEmail = "" | vUserCode = "system")
		putitem/id pOut, "Status", -1
		return(0)
	endif
	
	clear/e "CN_USER"
	USER_CODE.CN_USER = vUserCode
	retrieve/e "CN_USER"
	if($procerror = &lt;UACT_SUCCESS&gt;)
		if(vUserEmail = $lowercase(PRIMARY_EMAIL.CN_USER))
			if ($hits("CN_USER_RELATIONSHIP") &gt; 0)
				call lpResetPassword()
				putitem/id pOut, "Status", 0
			else
				putitem/id pOut, "Status", -1 
			endif
		else
			putitem/id pOut, "Status", -1
		endif
	endif
	
	return (0)
	
end




operation UserPermissions
	params
		struct pParams : INOUT
	endparams
	variables
		string 		vUserCode, 		vPartnerStatus,	vFunctionList
		numeric 	vCompanyType, 	vPartnerType, 	vPermissionCode
	endvariables
	
	vUserCode 			= pParams-&gt;in-&gt;UserCode
	vPartnerStatus 		= pParams-&gt;in-&gt;PartnerStatus
	vCompanyType 		= pParams-&gt;in-&gt;CompanyType
	vPartnerType 		= pParams-&gt;in-&gt;PartnerType
	
	if (vUserCode = "" | vPartnerStatus = "" | vCompanyType = "" | vPartnerType = "")
		pParams-&gt;out-&gt;Status = -99
		return(0)
	endif
	
	clear/e "CN_USER"
	USER_ID.CN_USER = vUserCode
	retrieve/e "CN_USER"
	if ($procerror != &lt;UACT_SUCCESS&gt;)
		pParams-&gt;out-&gt;Status = "Unknown User"
		return(0)
	endif

	if ($totocc(CN_USER_FQS_SYSTEM) != 1)
		pParams-&gt;out-&gt;Status = "FQS_SYSTEM no records"
		return(0)
	endif

;	forentity "CN_USER_FQS_SYSTEM"
	if (DEFAULT_VALUE.CN_USER_FQS_SYSTEM = "" | DEFAULT_VALUE.CN_USER_FQS_SYSTEM &lt; 1)
		pParams-&gt;out-&gt;Status = "FQS_SYSTEM not set"
		return(0)
	endif
;	endfor	

	vFunctionList = ""

	if (vCompanyType = 0)	; Partner Company	
		selectcase vPartnerStatus 
			case "X"     				; CLOUDIES
				call lpSetCloudies(vFunctionList)
			case "P"     				; NETWORK PARTNERS
				call lpSetPartners(vFunctionList,DEFAULT_VALUE.CN_USER_FQS_SYSTEM)
			elsecase      				; NETWORK ASSOCIATES
				call lpSetPartners(vFunctionList)
		endselectcase
	else 	; client
	endif

	pParams-&gt;out-&gt;FunctionList 	= vFunctionList
	pParams-&gt;out-&gt;Status 			= ""

end

