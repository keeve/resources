entry lpResetPassword
variables
	string vPassword
endvariables
	;; Random Password generate
	vPassword = $uuid[1:8]

	while ($length(vPassword) &lt; 8)
		vPassword = $concat(vPassword, "J")
	endwhile

	if ($scan(vPassword, $syntax('&amp;')) = 0)
		vPassword = $concat("G", vPassword[2])
	endif

	if ($scan(vPassword, $syntax('#')) = 0)
		vPassword = $concat("4", vPassword[2])
	endif

	;; Set Password 
	PASSWORD.CN_USER = encodePassword($uppercase(vPassword))
	LAST_UPDATED_BY.CN_USER    = "PWDRESET"
	LAST_UPDATED.CN_USER    = $datim 
	PASSWORD_LAST_UPDATED.CN_USER  = $datim 

	store/e "CN_USER"
	if ($procerror = &lt;UACT_SUCCESS&gt;)
		commit
		call sendEmail($uppercase(vPassword))
		;filedump/append "%%$datim%%% Reset Pwd : %%USER_CODE.CN_USER%%% : Encoded:%%PASSWORD.CN_USER%%% Decoded:%%vPassword%%%%%^","logins.txt" 
	else
		rollback
		call sendEmailPasswordFailed(vPassword)
	endif
end


entry sendEmail
params 
	string pPassword : In
endparams
variables
	struct vStruct
	string	vDate, vTime, vBody
endvariables
		
		vDate			=  $concat($date[D],"-",$date[M],"-",$date[Y])
		vTime			= $concat($clock[H], ":", $clock[N])
		
		vBody   		= $mail
		vBody   		= $replace(vBody, 1, "[Date]",vDate, -1)
		vBody   		= $replace(vBody, 1, "[Time]",vTime, -1)
		vBody			= $replace(vBody, 1, "[user]",FIRST_NAME.CN_USER, -1)
		vBody			= $replace(vBody, 1, "[password]",pPassword, -1)
		
		
		#include IFC:initStructParam
		vStruct-&gt;in-&gt;From = $logical("FromEmailDomain")
		vStruct-&gt;in-&gt;To = PRIMARY_EMAIL.CN_USER		
		vStruct-&gt;in-&gt;Priority = 1
		vStruct-&gt;in-&gt;CopySelf = 0
		vStruct-&gt;in-&gt;MIME = "HTML" 
		vStruct-&gt;in-&gt;Subject = "FQS – Password Reset Request"
		vStruct-&gt;in-&gt;User = USER_ID.CN_USER
		vStruct-&gt;in-&gt;Body = vBody

		activate "S_SY_SA_EMAILER".SendEmail(vStruct)
		return(0)

		mail:blockdata +
		&lt;body style="font-family:'Calibri';"&gt;
			&lt;h3 style="background:#d9edf7;text-align:center;color:#3a87ad"&gt;You submitted for a password reset&lt;/h3&gt;
			&lt;p&gt;Dear [user]&lt;/p&gt;
			&lt;p&gt;Your new password is &lt;b&gt;[password]&lt;/b&gt;. &lt;br /&gt; It is recommended to change your password immediately into something that you will remember upon login. &lt;/p&gt;
			&lt;p&gt;To change your password, click [My Profile] from the [Administration] menu. &lt;/p&gt;
			&lt;p&gt;&lt;i&gt;FQS&lt;/i&gt;&lt;/p&gt;
			&lt;i&gt;This email was auto-generated by FQS on [Date] at [Time] and sent from an unattended inbox, please do not reply.&lt;/i&gt;
		&lt;/body&gt;
		+
end


entry sendEmailPasswordFailed
params 
	string pPassword : In
endparams
variables
	struct vStruct
	string	vFromAddr, vToAddr, vDate, vTime, vBody, vSubject
endvariables
		vFromAddr	=	"no-reply@freightquotesystem.com.au"
		vToAddr		=	PRIMARY_EMAIL.CN_USER
		vDate			=  $concat($date[D],"-",$date[M],"-",$date[Y])
		vTime			= 	$concat($clock[H], ":", $clock[N])
		vSubject		= 	"FQS – password reset request"
		vBody   		= $mail
		vBody   		= $replace(vBody, 1, "[Date]",          					vDate,				-1)
		vBody   		= $replace(vBody, 1, "[Time]", 								vTime,				-1)
		vBody			= $replace(vBody, 1, "[user]",								USER_CODE.CN_USER,	-1)
		vBody			= $replace(vBody, 1, "[changedUser]",						&lt;userName&gt;,			-1)
		vBody			= $replace(vBody, 1, "[password]",							pPassword,			-1)
;		activate "EMAIL_BOX".SendEmailwAttch(vFromAddr, vToAddr, "", "itsupport@ifc.com.au", vSubject, vBody, "", "System", "HTML", 3, 0, "", "")
		return(0)

		mail:blockdata +
		&lt;body style="font-family:'Calibri';"&gt;
			&lt;h3 style="background:green;text-align:center;color:#fff"&gt;A request has been submitted to reset your password&lt;/h3&gt;
			&lt;p&gt;Dear User&lt;/p&gt;
			&lt;p&gt;
				Your password for [user] has been reset via the password reset screen. The new password is [password] and will need resetting when next logging in&lt;/p&gt;
			&lt;p&gt;
				FreightSupplyChain.com.au
			&lt;/p&gt;
			&lt;i&gt;This email was auto-generated by FSC on [Date] at [Time] and sent from an unattended inbox, please do not reply.&lt;/i&gt;
		&lt;/body&gt;
		+
end


