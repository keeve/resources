entry ReceiveEmail
	params
		string        pAttachDir    : IN
		string        vUserName    : IN
		string        vPassword    : IN
		string        pMailList    : OUT
		boolean        pSuccess        : OUT
		string        pRetDesc        : OUT
		String pSearchSubject : IN
		string pSearchBody : IN
	endparams
	variables
		handle        vMailApi
		numeric        vTotalMails,    vCount,            vCount2,            vLastCount
		string        vEnv,             vMailServer,    vEmailList,        vEmailListPart,    vOcc
		string        vHeaderList,    vMailText,        vMailTextPart,    vAttList,             vMailSize
		string        vAttachDir
		numeric        vPos
		string        vUserFirst
	endvariables
	pSuccess    = 1
	vEnv        = $logical("ENV")
	vMailServer = $logical("PopMailServer")
	; Mantis 1121 Starts
	;    vUserName   = $logical("365User")
	;    vPassword   = $logical("365Pass")
	; Mantis 1121 Ends
	vTotalMails = 0
	
	if (pAttachDir = "")
		pAttachDir = $logical("RCVEMLEmailAttTemp")
	endif
	
	vPos = $rscan(vUserName, "@")
	vUserFirst = vUserName[1:vPos - 1]
	
	pAttachDir = $concat(pAttachDir, vUserFirst, "\")
	ldircreate pAttachDir 
	pAttachDir = $concat(pAttachDir, $concat($date,"-",$clock), "\")
	ldircreate pAttachDir 
	
	
	; Create an instance handle of the UPOPMAIL API component
	newinstance "UPOPMAIL", vMailApi
	
	vMailApi-&gt;LogonMail(vMailServer, vUserName, vPassword)
	if ($status &lt; 0)
		pSuccess = 0
		pRetDesc = "%%$datim%%% Log on failed:  Status %%$status : Mail Server %%vMailServer%%% : User %%vUserName%%% (%%vPassword%%%)%%^"
filedump/append pretDesc, "eMail Uploader Log.txt"
		return(0)
	else
		vMailApi-&gt;ListMail(vEmailList)
		if ($status = -16)                                                        ; -16 indicates that data exceeds 10K
			repeat
				vMailApi-&gt;ListMailX (vEmailListPart)
				if (vEmailList != "")
					vEmailList = $concat(vEmailList, vEmailListPart)    ; Add the data to the existing email list 
				else
					vEmailList = vEmailListPart
				endif
			until ($status != -16)
		else
			;            pRetDesc = $concat(pRetDesc,$datim, " vMailApi-&gt;ListMail(vEmailList) : $status : ",$status," %%^" )
			
		endif
		
		vTotalMails = $status
	endif
	
	;Retrieve the email messages 
	if (vTotalMails &gt; 0)
		pRetDesc = $concat(pRetDesc,$datim, " Identified ",vTotalMails," files to be retrieved%%^" )
		vCount2=1
		vLastCount = 0
		for vCount = 1 to vTotalMails
			if (vCount &lt; 50) ; only do 50 at a time
				vAttachDir = $concat(pAttachDir,vCount,"\")
				ldircreate vAttachDir 
				vMailApi-&gt;GetMail(vCount, vAttachDir, vMailSize, vHeaderList, vMailText, vAttList)
				if ($status = -16)
					repeat
						vMailApi-&gt;GetMailX(vCount, vAttachDir, vMailSize, vHeaderList, vMailTextPart, vAttList)
						vMailtext = $concat(vMailText, vMailTextPart)
					until ($status != -16)
					if ((pSearchSubject = "" | (pSearchSubject != "" &amp; $scan(vHeaderlist,pSearchSubject) &gt; 0)) |  (pSearchBody = "" | (pSearchBody != "" &amp; $scan(vMailText,pSearchBody) &gt; 0)))
						putitem/id vOcc, "HEADER",         vHeaderList
						putitem/id vOcc, "BODY",           vMailText
						putitem/id vOcc, "ATTACH",         vAttList
						putitem/id vOcc, "ATTACHDIR",    vAttachDir
						putitem pMailList, -1, vOcc
						pRetDesc = $concat(pRetDesc,$datim," (%%vCount%%%) Retrieve successful : ",$concat("FROM:",$item("FROM", vHeaderList)," SUBJECT:",$item("SUBJECT", vHeaderList))  ,"%%^" )
						if ($logical("ENV") = "IFCFSCPROD")
							vMailApi-&gt;DeleteMail(vCount)
						endif
					endif
				elseif($status = 3)
					; duplicate file so reset the Attach Dir and get this one again
					if (vLastCount = vCount)
						pRetDesc = $concat(pRetDesc,$datim," (%%vCount%%%) Duplicate file, unable to process : ",$concat("FROM:",$item("FROM", vHeaderList)," SUBJECT:",$item("SUBJECT", vHeaderList)) ,"%%^" )
					else
						vAttachDir = $concat(pAttachDir,vCount,"\",vCount2,"\")
						ldircreate vAttachDir
						vCount2    =  vCount2 +1
						vCount    =    vCount -1
						vLastCount = vCount
					endif 
				elseif($status &gt;= 0)
					;                pRetDesc = $concat(pRetDesc,$datim, " vMailApi-&gt;GetMail : $status : ",$status," %%^" )
					if ((pSearchSubject = "" | (pSearchSubject != "" &amp; $scan(vHeaderlist,pSearchSubject) &gt; 0)) |  (pSearchBody = "" | (pSearchBody != "" &amp; $scan(vMailText,pSearchBody) &gt; 0)))
						putitem/id vOcc, "HEADER",         vHeaderList
						putitem/id vOcc, "BODY",           vMailText
						putitem/id vOcc, "ATTACH",         vAttList
						putitem/id vOcc, "ATTACHDIR",    vAttachDir
						putitem pMailList, -1, vOcc
						pRetDesc = $concat(pRetDesc,$datim," (%%vCount%%%) Retrieve successful : ",$concat("FROM:",$item("FROM", vHeaderList)," SUBJECT:",$item("SUBJECT", vHeaderList))  ,"%%^" )
						if ($logical("ENV") = "IFCFSCPROD")
							vMailApi-&gt;DeleteMail(vCount)
						endif
					endif
				else
					pRetDesc = $concat(pRetDesc,$datim, " vMailApi-&gt;GetMail : $status : ",$status," %%^" )
					pRetDesc = $concat(pRetDesc,$datim," (%%vCount%%%) **Failed to retrieve eMail : ",$concat("FROM:",$item("FROM", vHeaderList)," SUBJECT:",$item("SUBJECT", vHeaderList)),"%%^" )
				endif
			endif
		endfor
	endif
	
	vMailApi-&gt;LogoffMail()
end ;ReceiveEmail





entry lpFormatDateTime
	params
		string    pDateTimeStrg        : IN
		datetime    pDateTime            : OUT
		string    pTimeZone            : OUT
	endparams
	variables
		string    vMonth, vDay, vYear, vHour, vMinute
		string    vDateTimeStrg
	endvariables
	
	if (pDateTimeStrg = "")
		pDateTime = ""
		pTimeZone = ""
	else
		vDateTimeStrg = pDateTimeStrg
		vMonth        = vDateTimeStrg[1:$scan(vDateTimeStrg, " ")-1]
		vDateTimeStrg = vDateTimeStrg[$scan(vDateTimeStrg, " ")+1]
		vDay          = vDateTimeStrg[1:$scan(vDateTimeStrg, " ")-1]
		vDateTimeStrg = vDateTimeStrg[$scan(vDateTimeStrg, " ")+1]
		vYear         = vDateTimeStrg[1:$scan(vDateTimeStrg, " ")-1]
		vDateTimeStrg = vDateTimeStrg[$scan(vDateTimeStrg, " ")+1]
		vHour         = vDateTimeStrg[1:$scan(vDateTimeStrg, ":")-1]
		vDateTimeStrg = vDateTimeStrg[$scan(vDateTimeStrg, ":")+1]
		vMinute       = vDateTimeStrg[1:2]
		vDateTimeStrg = vDateTimeStrg[3]
		if (vDateTimeStrg[1:2] = "PM")
			vHour = $number(vHour) + 12
		endif
		pTimeZone     = vDateTimeStrg[5:$scan(vDateTimeStrg, ")")-1]
		vDateTimeStrg = ""
		vDateTimeStrg = $concat($concat(vDay, "-", vMonth, "-", vYear[3:2]), vHour, ":", vMinute)
		pDateTime     = $datim(vDateTimeStrg)
	endif
	
	return(0)
	
end ;lpFormatDateTime



entry lpEmailLogFile
	params
		string    pLogFile    : IN
		string    pTo        : IN
		boolean    pSuccess    : OUT
	endparams
	variables
		boolean    vSuccess
		string    vStrDate
		string    vCSMEmail, vReturnTxt
		string    vFromAddr, vToAddr,  vCCAddr, vBCCAddr,  vSubject,   vBody
	endvariables
	
	if (pTo = "")
		return(0)
	endif
	
	activate "GENERAL_SVC".current_dateformat($date, vStrDate)
	
	vFromAddr = "DOXBatch@ifc.com.au"
	vToAddr   = pTo
	vCCAddr   = vCSMEmail
	
	vSubject  = $concat("FSC Received Email Processor LOG FILE - ", vStrDate)
	
	fileload pLogFile, vBody
	
	vBody      = $replace(vBody,1,"%%^","&lt;br&gt;",-1)
	vBody      = $replace(vBody,1," ","&amp;nbsp;",-1)
	
	activate "EMAIL_BOX".SendEmail(vFromAddr, vToAddr, vCCAddr, vBCCAddr, vSubject, vBody, "FSCBATCH", "HTML", 3, 0, vSuccess, vReturnTxt)
	if (vSuccess)
		pSuccess = 1
	else
		pSuccess = 0
	endif
	
	return(0)
	
end ;entry lpEmailLogFil





entry lpEmailNoAttachments
	params
		string    pSubject                : IN
		string    pBody                    : IN
		numeric    pPriority            : IN
		string    pFilePath            : IN
		string    pFiles                : IN
		boolean    pSuccess                : OUT
	endparams
	variables
		boolean    vSuccess
		string    vStrDate
		string    vReturnTxt
		string    vFromAddr, vToAddr,  vCCAddr, vBCCAddr,  vSubject,   vBody, vFrom
	endvariables
	
	;    activate "SYSIMPO_01".contact_person_info(pImpCode, "", vCSMName, "", "", "", "", "", "", "", "", vCSMEmail, "", "", "", "", "", vCSMTele, "", vCSMFax, "", "", "", "", "", "", "", "", "", "", "", "", "")
	;    activate "GKP_FSC_UTL".GetImpPref(pImpCode, "GKP-PO1", vEmailList)
	activate "GENERAL_SVC".current_dateformat($date, vStrDate)
	
	vFromAddr     = "DOXBatch@ifc.com.au"
	vToAddr       = "manager-dox@ifc.com.au"
	
	vBody         = "This email has been sent to you because no attachments were found &lt;br&gt;&lt;br&gt;"
	
	vSubject     = $item("SUBJECT", pSubject)
	vFrom             = $item("FROM", pSubject)
	
	vBody         = $concat(vBody,"&lt;br&gt;&lt;br&gt;ORIGINAL EMAIL:&lt;br&gt;===============&lt;br&gt;&lt;br&gt;Subject:%%vSubject%%%&lt;br&gt;From:%%vFrom%%%&lt;br&gt;",$replace(pBody,1,"%%^","&lt;br&gt;",-1))
	
	vSubject     = vSubject[1:125]
	
	activate "EMAIL_BOX".SendEmail(vFromAddr, vToAddr, vCCAddr, vBCCAddr, pSubject, vBody, "FSCBATCH", "HTML", pPriority, 0, vSuccess, vReturnTxt)
	if (vSuccess)
		pSuccess = 1
	else
		pSuccess = 0
	endif
	
	return(0)
	
end 




entry lpEmailExceptions
	params
		string    pSubject                : IN
		string    pBody                    : IN
		numeric    pPriority            : IN
		boolean    pOtherFiles            : IN
		string    pFilePath            : IN
		string    pFilesAttached        : IN
		string    pFiles                : IN
		boolean    pSuccess                : OUT
	endparams
	variables
		boolean    vSuccess
		string    vStrDate
		string    vReturnTxt
		string    vFromAddr, vToAddr,  vCCAddr, vBCCAddr,  vSubject,   vBody
		string    vFrom
	endvariables
	
	;    activate "SYSIMPO_01".contact_person_info(pImpCode, "", vCSMName, "", "", "", "", "", "", "", "", vCSMEmail, "", "", "", "", "", vCSMTele, "", vCSMFax, "", "", "", "", "", "", "", "", "", "", "", "", "")
	;    activate "GKP_FSC_UTL".GetImpPref(pImpCode, "GKP-PO1", vEmailList)
	activate "GENERAL_SVC".current_dateformat($date, vStrDate)
	
	pFiles        = $replace(pFiles,1,"%%^","&lt;br&gt;",-1)
	vFromAddr     = "DOXBatch@ifc.com.au"
	vToAddr       = "dox-exceptions@ifc.com.au"
	
	vBody         = "The following files were not recognised (this may be because the sender has forwarded emails as attachments, please check):-&lt;br&gt;&lt;br&gt;"
	vBody            = "%%vBody%%%&lt;br&gt;%%pFiles%%%"
	
	if (pOtherFiles)
		vBody        = "%%vBody%%%&lt;br&gt;&lt;br&gt;Please note, other recognised files were attached to the original email and these have been processed separately and allocated to the relevant email queue"
	endif
	
	vSubject     = $item("SUBJECT", pSubject)
	vFrom             = $item("FROM", pSubject)
	
	vBody         = $concat(vBody,"&lt;br&gt;&lt;br&gt;ORIGINAL EMAIL&lt;br&gt;===============&lt;br&gt;&lt;br&gt;Subject:%%vSubject%%%&lt;br&gt;From:%%vFrom%%%&lt;br&gt;",$replace(pBody,1,"%%^","&lt;br&gt;",-1))
	
	vSubject     = vSubject[1:125]
	
	activate "EMAIL_BOX".SendEmailwAttch(vFromAddr, vToAddr, vCCAddr, vBCCAddr, vSubject, vBody, pFilesAttached, "FSCBATCH","HTML", pPriority, 0, vSuccess, vReturnTxt)
	if (vSuccess)
		pSuccess = 1
	else
		pSuccess = 0
	endif
	
	return(0)
	
end 


entry lpEmailToSender
	params
		string    pType                    : IN
		string    pEmailTO                : IN
		string    pEmail                : IN
		string    pSubject                : IN
		string    pBody                    : IN
		numeric    pPriority            : IN
		string    pFiles                : IN
		boolean    pSuccess                : OUT
	endparams
	variables
		boolean    vSuccess
		string    vStrDate
		string    vReturnTxt, vEmailList
		string    vFromAddr, vToAddr,  vCCAddr, vBCCAddr, vBody
	endvariables
	
	activate "GENERAL_SVC".current_dateformat($date, vStrDate)
	
	vFromAddr     = pEmail
	vToAddr       = pEmailTO
	vCCAddr        = vEmailList
	
	selectcase pType
		case "1"
			vBody    = "Thankyou for using the PO/GI Auto-processor.  Unfortunately, I don't recognise the file type you have attached.  Please resubmit the correct file.&lt;br&gt;&lt;br&gt;"
			vBody    = $concat(vBody,"&lt;br&gt;&lt;br&gt;THE FOLLOWING FILES HAVE NOT BEEN PROCESSED&lt;br&gt;==========================================&lt;br&gt;&lt;br&gt;",pFiles,"&lt;br&gt;&lt;br&gt;")
			if (pBody != "")
				vBody = $concat(vBody,"&lt;br&gt;&lt;br&gt;ORIGINAL EMAIL&lt;br&gt;===============&lt;br&gt;&lt;br&gt;",pBody)
			endif
		case "2"                
			vBody = "Thankyou for using the PO/GI Auto-processor.  Unfortunately, there were no files attached to your original email.  Please resubmit with a file in the correct format.&lt;br&gt;&lt;br&gt;"
			if (pBody != "")
				vBody = $concat(vBody,"&lt;br&gt;&lt;br&gt;ORIGINAL EMAIL&lt;br&gt;===============&lt;br&gt;&lt;br&gt;",pBody)
			endif
		case "3"
			vBody = "Thankyou for using the PO/GI Auto-processor.  Unfortunately, I was unable to determine what process to run based on the subject and your email address.  Please correct and then resubmit your request.&lt;br&gt;&lt;br&gt;"
			vBody    = $concat(vBody,"&lt;br&gt;&lt;br&gt;THE FOLLOWING FILES HAVE NOT BEEN PROCESSED&lt;br&gt;=======================================&lt;br&gt;&lt;br&gt;",pFiles,"&lt;br&gt;&lt;br&gt;")
			if (pBody != "")
				vBody = $concat(vBody,"&lt;br&gt;&lt;br&gt;ORIGINAL EMAIL&lt;br&gt;===============&lt;br&gt;&lt;br&gt;",pBody)
			endif
		case "*"
			vBody = "Thankyou for using the PO/GI Auto-processor. &lt;br&gt;&lt;br&gt;"
			vBody    = $concat(vBody,"&lt;br&gt;&lt;br&gt;THE FOLLOWING FILES HAVE BEEN PASSED TO THE BATCH COMPONENT FOR PROCESSING&lt;br&gt;==================================================&lt;br&gt;&lt;br&gt;",pFiles,"&lt;br&gt;&lt;br&gt;")
		elsecase
			vBody = "Thankyou for using the PO/GI Auto-processor.  Unfortunately, I was unable to determine what process to run based on the subject and your email address.  Please correct and then resubmit your request.&lt;br&gt;&lt;br&gt;"
			vBody    = $concat(vBody,"&lt;br&gt;&lt;br&gt;THE FOLLOWING FILES HAVE NOT BEEN PROCESSED&lt;br&gt;========================================&lt;br&gt;&lt;br&gt;",pFiles,"&lt;br&gt;&lt;br&gt;")
			if (pBody != "")
				vBody = $concat(vBody,"&lt;br&gt;&lt;br&gt;ORIGINAL EMAIL&lt;br&gt;===============&lt;br&gt;&lt;br&gt;",pBody)
			endif
	endselectcase
	
	
	activate "EMAIL_BOX".SendEmailwAttch(vFromAddr, vToAddr, vCCAddr, vBCCAddr, pSubject, vBody, pFiles, "FSCBATCH","HTML", pPriority, 0, vSuccess, vReturnTxt)
	if (vSuccess)
		pSuccess = 1
	else
		pSuccess = 0
	endif
	
	return(0)
	
end
