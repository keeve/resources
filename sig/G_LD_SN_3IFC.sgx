<DAT name="SFUNC" xml:space='preserve'>operation GetMail
	variables
		boolean    vSuccess,        vFailed
		numeric    vPos
		string    vAttachDir,        vOcc,          vMailList,        vHeader,                vStatRpts,    vImpCode
		string    vFile,            vNewFile,    vExt,              vAttList,            vName
		numeric    vEDIDir,            vPriority
		string    vEDIPDFs,        vEDIXMLs,    vLOGPath,        vLogFile,            vPDFFiles,    vBody
		string    vQueueName,        vXMLFiles,    vExceptFiles,    vDoxSplitter
		string    vEDIFileLoc,                    vFileAttachDir,                        vExceptFileLoc
		boolean    vPDFsExist,        vMantisOK,    vXMLsExist,        vExceptExist
		boolean    vExceptOK,         vPostEDIExist
		boolean    vEmailLogOK,    vSenderOK
		string    vMailBoxList,                     vMailBox,         vEMLUser,            vEMLPassword
		string    vPostEDIFiles, vPOXMLFilePath,    vPOCSVFilePath
		string    vPOForwarder,                    vPOFileType,    vImp
		string    vQueuePriority,                vEmailFailMsg,    vManagerEmail,     vMantisFilePath, vMantisFileLoc
		string    vGIXMLFilePath,                vGIFileType,    vGIForwarder
		string    vMantisFileLocPath,            vEDIFileLocPath,                        vExceptFileLocPath
		string    vPDFActualFiles,                vXMLActualFiles,                        vExcActualFiles
		string    vPosActualFiles,                vEmailMsg,                                vFileToAttach 
		numeric    vCountBoxes,                     vCountBoxesEmpty
		string    vITSupport,    vMantis, vGRFileType, vActivate, vComponent, vNewFileFullPath
		; mantis 1564 starts
		string    vPRDXMLFilePath, vPRDCSVFilePath,     vPRDForwarder,vGRXMLFilePath,vGRForwarder,VLOGFILEMGR, vGKPPOXFilePath
		; mantis 1564 ends
		boolean    vDox
		
		; mantis 1020739
		string         vFilePath
		Boolean         vCSVOK
		string vfrom, vreclist, vscan, vsubject,vemail, vcontactno,vcountry,vcity,vstate, vnotes, vsellercode,vcompanyname, vMessageBody
		numeric vemaillen
		String    vString
	endvariables
	
	
	$FilePath$ = $logical("GKP_EML")    
	vLogPath             = $concat($FilePath$, "log\")
	vLogFile             = $concat(vLOGPath, $datim," Log.txt")
	vLogFileMgr           = $concat(vLOGPath, $datim," LogMgr.txt")
	
	filedump/append $concat($datim, " ***************************************************************%%^"), vLogFile 
	filedump/append $concat($datim, " Starting %%^"), vLogFile 
	
	vEMLUser            = $item("USER", vMailBox)
	vEMLPassword        = $item("PWD", vMailBox)
	filedump/append $concat($datim, " ***********Retrieving ",vEMLUser,"*********%%^"), vLogFile 
	
	
	call ReceiveEmail(vAttachDir, "fqs@ifc.com.au", "Mugg1e$mugg1es!*", vMailList, vSuccess, vEmailFailMsg,"IFC Website Enquiry","")
	if (vSuccess)
		if (vMailList = "")
			vCountBoxesEmpty        = vCountBoxesEmpty + 1    
			if (vEmailFailMsg != "")
				filedump/append vEmailFailMsg,vLogFile
			endif
			filedump/append $concat($datim, " No emails to process%%^"), vLogFile 
		else
			if (vEmailFailMsg != "")
				filedump/append vEmailFailMsg,vLogFile
			endif 
			
			filedump/append $concat($datim, " ***************************************************************%%^"), vLogFileMgr
			filedump/append $concat($datim, " Starting %%^"), vLogFileMgr
			filedump/append $concat($datim, " " , $itemcount(vMailList)," email(s) retrieved for process%%^"), vLogFileMgr
			
			filedump/append $concat($datim, " " , $itemcount(vMailList)," email(s) to process%%^"), vLogFile 
			
			forlist vOcc in vMailList
				vHeader                         = $item("HEADER", vOcc)
				vBody                          = $item("BODY", vOcc)
				vFileAttachDir                   = $item("ATTACHDIR", vOcc)
				vQueueName                        = ""
				$FROM_EMAIL$                     = $item("FROM", vHeader) 
				vBody                         = $replace(vBody, 1, 'From: ', "FROM=", -1)
				vBody                         = $replace(vBody, 1, 'Subject: ', "&uSEP;SUBJECT=", -1)
				vBody                         = $replace(vBody, 1, 'Contact Number: ', "&uSEP;CONTACTNO=", -1)
				vBody                         = $replace(vBody, 1, 'Company Name: ', "&uSEP;COMPANY=", -1)
				vBody                         = $replace(vBody, 1, 'City: ', "&uSEP;CITY=", -1)
				vBody                         = $replace(vBody, 1, 'State: ', "&uSEP;STATE=", -1)
				vBody                         = $replace(vBody, 1, 'Country: ', "&uSEP;COUNTRY=", -1)
				vBody                         = $replace(vBody, 1, 'Message Body:', "&uSEP;MESSAGE=", -1)
				vBody                         = $replace(vBody, 1, '&uSEP; ', "&uSEP;", -1)    
				
				vSubject                        = $replace($item("SUBJECT", vBody),1,"%%^","",-1)
				vSubject                        = $replace($item("SUBJECT", vBody),1,"    ","  ",-1)
				vMessageBody                    = $replace($item("MESSAGE", vBody),1,"%%^","&lt;br&gt;",-1)
				vMessageBody                    = $replace($item("MESSAGE", vBody),1,"    ","  ",-1)
				vMessageBody                    = $ltrim(vMessageBody, "%%^")
				vNotes                            = $concat("&lt;b&gt;",$ltrim($rtrim(vSubject, " ")," "),"&lt;/b&gt;",$ltrim($rtrim(vMessageBody, " ")," "))
				
				vEmailLen                         = $rscan(vNotes,"--")
				vNotes                            = vNotes[1:vEmailLen-1]
				vNotes                            = $rtrim(vNotes, "%%^")
				
				vBody                            = $replace(vBody, 1, "%%^", "",-1)
				vBody                            = $replace(vBody, 1, "    ", "  ",-1)
				
				$result                        = $split($item("FROM", vBody), 1, "&lt;", vName, vEmail)
				vEmail                         = $replace(vEmail, 1, "&gt;", "")
				vEmail                            = $ltrim($rtrim(vEmail, " ")," ")
				vEmail                            = $replace(vEmail, 1, "%%^", "")
				vName                            = $ltrim($rtrim(vName, " ")," ")
				vContactNo                     = $ltrim($rtrim($item("CONTACTNO", vBody), " ")," ")                
				vCity                            = $ltrim($rtrim($item("CITY", vBody), " ")," ")                
				vState                            = $ltrim($rtrim($item("STATE", vBody), " ")," ")
				vCountry                       = $ltrim($rtrim($item("COUNTRY", vBody), " ")," ")         
				vCompanyName                      = $ltrim($rtrim($item("COMPANY", vBody), " ")," ")         
				
				
				clear/e "CN_LEADS"
				LEAD_UUID.CN_LEADS             = $uuid
				retrieve/e "CN_LEADS"
				
				SELLER_CODE.CN_LEADS             = "FA5F8E48-C351-48D4-9514-2DB9072BA8A0"
				clear/e "CN_COMPANY_DEFAULTS"
				COMPANY_ID.CN_COMPANY_DEFAULTS    = "FA5F8E48-C351-48D4-9514-2DB9072BA8A0"
				DEFAULT_CODE.CN_COMPANY_DEFAULTS    = "LEADSUSER"
				retrieve/e "CN_COMPANY_DEFAULTS"
				OWNER_UUID                        = DEFAULT_VALUE.CN_COMPANY_DEFAULTS
				ASSIGNED_TO.CN_LEADS            = DEFAULT_VALUE.CN_COMPANY_DEFAULTS
				
				NAME.CN_LEADS                    = vName
				EMAIL.CN_LEADS                    = vEmail
				COMPANY_NAME.CN_LEADS            = vCompanyName
				CONTACT_NO.CN_LEADS            = vContactNo
				CITY.CN_LEADS                    = vCity
				STATE.CN_LEADS                    = vState
				COUNTRY.CN_LEADS                = vCountry
				NOTES.CN_LEADS                    = vNotes
				STATUS.CN_LEADS                 = "N"
				CREATED_BY.CN_LEADS            = "EMAIL_BATCH"
				CREATED_ON.CN_LEADS            = $datim
				clear/e "CN_COMPANY_DEFAULTS"
				COMPANY_ID.CN_COMPANY_DEFAULTS    = "FA5F8E48-C351-48D4-9514-2DB9072BA8A0"
				DEFAULT_CODE.CN_COMPANY_DEFAULTS    = "LEADSUSER"
				retrieve/e "CN_COMPANY_DEFAULTS"
				if ($procerror = &lt;UACT_SUCCESS&gt;)
					PRIVACY.CN_LEADS                = "I"
					
					LAST_UPDATED_BY.CN_LEADS        = "EMAIL_BATCH"
					LAST_UPDATED.CN_LEADS            = $datim
				endif
					store/e "CN_LEADS"
					if ($procerror = &lt;UACT_SUCCESS&gt;)
						commit
					else
						filedump/append $concat($datim, " " , " failed to store CN_LEADS%%^"), vLogFile 
						rollback
					endif
					
					
					activate $instancename.createCSV($EMAIL_BODY$, vCSVOK) ; mantis 1020739
					if (vCSVOK)
						filedump/append $concat($datim, " CSV Completed%%^"), vLogFileMgr    
					else
						filedump/append $concat($datim, " CSV not processed%%^"), vLogFileMgr    
					endif
				endfor
				
			endif
			filedump/append $concat($datim, " ***************************************************************%%^"), vLogFile 
			
		else
			vFailed            = 1
			filedump/append $concat($datim, " ",vEmailFailMsg,"%%^"), vLogFile 
		endif
		
		
		filedump/append $concat($datim, " Finished%%^"), vLogFile 
		filedump/append $concat($datim, " ***************************************************************%%^"), vLogFile 
		
		
		if (vCountBoxes != vCountBoxesEmpty)
			vLogFile = $replace(vLogFile , 1, "%%^","&lt;br&gt;",-1)
			
			if (vFailed)
				call lpEmailLogFile(vLogFile,vMantis,vEmailLogOK)
			else
				call lpEmailLogFile(vLogFile,vITSupport,vEmailLogOK)
			endif    
		endif
		
		return(0)
	end 
	
	</DAT>
